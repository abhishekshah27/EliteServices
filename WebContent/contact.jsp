	<!-- contact -->
	<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3671.654912947716!2d72.54580381450842!3d23.036439484946033!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x395e84eb2ad9d19d%3A0xf2b4a864174675d9!2sK+S+School+Of+Business+Management!5e0!3m2!1sen!2sin!4v1552500659756" width="600" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
					   
	<section class="wthree-row py-5 w3-contact" id="contact">
	<div class="w3-main">
	
		
			<h4 class="w3ls-title text-center text-uppercase pb-md-5 pb-4"><u>contact Form</u></h4>
			<div class="container"><br/>
			
			<div class="agile-info">
			
				
                        
					<!-- contact form grid -->
					
						 <form action="contact" method="post">
							<div class="form-group">
								<label>Name</label>
								<input type="text" name="name" class="form-control" maxlength="30" required>
							</div>

							<div class="form-group">
								<label>Mobile no</label>
								<input type="text" name="phone" class="form-control" maxlength="10" pattern="[6-9]{1}[0-9]{9}" required>
							</div>
														
							<div class="form-group">
								<label>Email</label>
								<input type="email" name="email" class="form-control" maxlength="30" required>
							</div>
							
							<div class="form-group">
								<label>Your Message</label>
								<textarea name="message" class="form-control" rows="4" cols="50" maxlength="300" required></textarea>
							</div>
							
						
							<button type="submit"  class="btn btn-info btn-block1">Submit</button>
								
						</form>
						
						
					</div>
					</div>
				</div>
				
	</section>
	</section>
